  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "react": "16.4.1",
    "react-bootstrap": "0.32.1",
    "react-dom": "16.4.1",
    "remotedev": "0.2.7"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "concurrently": "3.6.0",
